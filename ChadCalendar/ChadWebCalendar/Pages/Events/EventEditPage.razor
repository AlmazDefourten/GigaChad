@page "/eventedit/{input}"
@using ChadWebCalendar.Components.Events;
@inject Data.Services.EventService eventService

<EventEdit _eventId="_eventId"></EventEdit>

@code {
    [Parameter]
    public string input { get; set; }
    public int _eventId { get; set; }
    protected override void OnParametersSet()
    {
        //the param will be set now
        _eventId = Convert.ToInt32(input);
    }
}
